{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/v1/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/public/(.*)",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}